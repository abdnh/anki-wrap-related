[MASTER]
ignore=forms
extension-pkg-whitelist=PyQt6,PyQt5

[REPORTS]
output-format=colorized

[MESSAGES CONTROL]
disable=
  line-too-long,
  missing-function-docstring,
  missing-module-docstring,
  missing-class-docstring,
  too-few-public-arguments,
  unused-argument,
  wildcard-import,
  unused-wildcard-import,
  too-few-public-methods,
  bare-except,
  too-many-arguments,

[BASIC]
good-names =
  i,
  mw,
